{
    "name": "zbateson/gpg-pear",
    "description": "Implementation of zbateson/gpg-interface using pear/Crypt_GPG",
    "type": "library",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Zaahid Bateson"
        }
    ],
    "require": {
        "php": ">=5.4",
        "pear/crypt_gpg": "^1.6",
        "guzzlehttp/psr7": "^1.0",
        "zbateson/gpg-interface": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8 || ^5.7 || ^6.5"
    },
    "autoload": {
        "psr-4": {
            "ZBateson\\GpgPear\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZBateson\\GpgPear\\": "tests/GpgPear"
        }
    }
}
